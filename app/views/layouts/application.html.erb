<!DOCTYPE html>
<html>
<head>
  <title>Genial</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<!-- Shell -->	
<div class="shell">
  
  <div id="header">
    <h1 id="logo">
      <a href="#">Genial Indumentaria</a>
    </h1>
    <!-- Navigation -->
    <%= render :partial => 'store/nav' %> 
  </div>  
  <!-- Main -->
  <div id="main">
    <%= yield %>
  </div>
  <!-- End main -->    
</div>	
<!-- End Shell -->    
</body>
</html>
